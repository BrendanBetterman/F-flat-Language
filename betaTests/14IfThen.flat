//  14IfThen.flat

//  Tests nested fif statement with felse clauses.

BOF

	int n, m;	

	foutln( "Enter two integers: ");
	fin( n, m);
	fif (n > m )
		foutln( n, " is greater than ", m, "\n");
	felse 
		fif( n < m )
			foutln( n, " is less than ", m, "\n");
		felse
			foutln( n, " is equal to ", m, "\n");
		fendif
	fendif
	foutln( "\n");

EOF
